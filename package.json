{
  "name": "starter-app",
  "version": "0.3.0",
  "description": "React starter kit for building modern Web Apps",
  "private": true,
  "author": "Abhishek Raj <abhi@raj.me>",
  "scripts": {
    "webpack": "webpack --config starter/config/webpack/webpack.config.ts",
    "build:client": "npm run webpack",
    "build:server": "cross-env PLATFORM=server npm run webpack",
    "build:client:watch": "npm run webpack -- --watch",
    "build:server:watch": "cross-env PLATFORM=server npm run webpack -- --watch",
    "build:all": "run-p --silent build:client build:server",
    "build:all:watch": "run-p --silent build:client:watch build:server:watch",
    "build": "npm run -s clean && npm run build:all",
    "launch:server": "nodemon --quiet --watch build/server --exec 'node build/server/index.js'",
    "launch:server:wait": "wait-on build/server/index.js && npm run launch:server",
    "start:all": "run-p --silent build:all:watch launch:server:wait type-check:watch",
    "start": "npm run -s clean && npm run start:all",
    "build-ts": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "clean": "npx rimraf build node_modules/.cache",
    "clean:all": "npx rimraf build node_modules package-lock.json",
    "blame": "git log -S",
    "hist": "git log --oneline | grep -i",
    "follow": "git log --follow",
    "blist": "npx browserslist",
    "blist:coverage": "npx browserslist --coverage",
    "blist:update": "npx browserslist@latest --update-db",
    "ncu": "ncu",
    "ncuu": "ncu -u",
    "check-env": "node -e 'console.log(process.env)' | grep npm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baadal/starter-app.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/node": "^16.0.0",
    "@types/nodemon": "^1.19.0",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.6",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.3.1",
    "nodemon": "^2.0.8",
    "npm-check-updates": "^11.7.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4",
    "wait-on": "^6.0.0",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "core-js": "^3.15.2"
  },
  "browserslist": [
    "cover 97%",
    "chrome >= 23",
    "firefox >= 21",
    "edge >= 12",
    "safari >= 6",
    "ios_saf >= 6",
    "opera >= 15",
    "not ie 11",
    "not dead"
  ]
}
